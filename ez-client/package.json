    "react-dom": "^15.4.2",
    "react-grid-layout": "^0.14.6"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "json-server": "^0.9.4",
    "knacss": "^6.0.7",
    "node-sass": "^4.3.0",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^4.1.1",
